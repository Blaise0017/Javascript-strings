          sayHello('Hello', 'Blaise');
       

         function sayHello(greeting, name = 'jake') {
            alert(greeting+ ' ' +name);
       }
        sayHello('Hello');

     function getsum(a, b){
         alert(a + b);
     }

     getsum(4, 5);

     function getsum(a, b){
         return a + b;
     }

    var sum = getsum(4, 5);
    console.log('the answer is '+sum);

